{
  "name": "@rotki/ui-library",
  "version": "2.12.0",
  "packageManager": "pnpm@10.27.0",
  "description": "A vue design system and component library for rotki",
  "type": "module",
  "keywords": [
    "vue",
    "ui",
    "components",
    "rotki"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/rotki/ui-library#readme",
  "bugs": {
    "url": "https://github.com/rotki/ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotki/ui-library.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "/dist"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.js",
      "default": "./dist/theme/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js",
      "default": "./dist/composables/index.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin/index.d.ts",
      "import": "./dist/vite-plugin/index.js",
      "default": "./dist/vite-plugin/index.js"
    },
    "./vite-plugin/client": {
      "types": "./dist/vite-plugin/client.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "theme": [
        "./dist/theme/index.d.ts"
      ],
      "composables": [
        "./dist/composables/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "vite-plugin": [
        "./dist/vite-plugin/index.d.ts"
      ],
      "vite-plugin/client": [
        "./dist/vite-plugin/client.d.ts"
      ]
    }
  },
  "scripts": {
    "build:prod": "tsx scripts/dist-build.mjs",
    "build": "pnpm run generate-icons && vite build && cp ../../README.md dist/ && cp src/vite-plugin/client.d.ts dist/vite-plugin/",
    "build:storybook": "STORYBOOK=true storybook build",
    "build:tailwind": "tailwindcss -o dist/style.css --minify",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "build:web-types": "tsx scripts/build-web-types.mjs",
    "generate-icons": "tsx scripts/generate-icons.mjs",
    "resolve:alias": "tsc-alias --replacer ./scripts/tsc-alias-replacer.cjs -p tsconfig.build.json --dir dist",
    "dev": "vite",
    "prepare": "node scripts/prepare.mjs",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:preview": "npx vite preview --open --outDir tests/coverage",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json",
    "new": "tsx scripts/new-story.ts"
  },
  "peerDependencies": {
    "@vueuse/core": ">13.0.0",
    "@vueuse/shared": ">13.0.0",
    "dayjs": ">=1.11.13 <12",
    "vue": ">=3.5.0"
  },
  "web-types": "dist/web-types.json",
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "scule": "1.3.0",
    "tinycolor2": "1.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/types": "7.28.5",
    "@fontsource/roboto": "5.2.9",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/addon-themes": "10.1.11",
    "@storybook/vue3-vite": "10.1.11",
    "@tsconfig/node24": "24.0.3",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.4",
    "@types/tinycolor2": "1.4.6",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "@vueuse/core": "14.1.0",
    "@vueuse/math": "14.1.0",
    "@vueuse/shared": "14.1.0",
    "argparse": "2.0.1",
    "autoprefixer": "10.4.23",
    "babel-loader": "10.0.0",
    "cac": "6.7.14",
    "consola": "3.4.2",
    "css-loader": "7.1.2",
    "fast-glob": "3.3.3",
    "fast-xml-parser": "5.3.3",
    "fs-extra": "11.3.3",
    "jsdom": "26.1.0",
    "lucide": "0.541.0",
    "msw": "2.12.7",
    "postcss": "8.5.6",
    "sass": "1.97.1",
    "storybook": "10.1.11",
    "tailwindcss": "3.4.19",
    "tsc-alias": "1.8.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "unplugin-auto-import": "20.3.0",
    "vite": "7.3.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vitest": "4.0.16",
    "vue": "3.5.26",
    "vue-component-meta": "3.2.1",
    "vue-loader": "17.4.2",
    "vue-router": "4.6.4",
    "vue-tsc": "3.2.1"
  },
  "engines": {
    "node": ">=24 <25",
    "pnpm": ">=10 <11"
  }
}
