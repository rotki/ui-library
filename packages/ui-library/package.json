{
  "name": "@rotki/ui-library",
  "version": "2.7.0",
  "packageManager": "pnpm@10.20.0",
  "description": "A vue design system and component library for rotki",
  "type": "module",
  "keywords": [
    "vue",
    "ui",
    "components",
    "rotki"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/rotki/ui-library#readme",
  "bugs": {
    "url": "https://github.com/rotki/ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotki/ui-library.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "/dist"
  ],
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.es.js",
      "default": "./dist/theme/index.es.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.es.js",
      "default": "./dist/components/index.es.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.es.js",
      "default": "./dist/composables/index.es.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "typesVersions": {
    "*": {
      "theme": [
        "./dist/theme/index.d.ts"
      ],
      "composables": [
        "./dist/composables/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build:prod": "tsx scripts/dist-build.mjs",
    "build": "pnpm run generate-icons && vite build",
    "build:storybook": "STORYBOOK=true storybook build",
    "build:tailwind": "tailwindcss -o dist/style.css --minify",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "build:web-types": "tsx scripts/build-web-types.mjs",
    "generate-icons": "tsx scripts/generate-icons.mjs",
    "resolve:alias": "tsc-alias --replacer ./scripts/tsc-alias-replacer.cjs -p tsconfig.build.json --dir dist",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:preview": "npx vite preview --open --outDir tests/coverage",
    "typecheck": "vue-tsc --noEmit",
    "new": "tsx scripts/new-story.ts"
  },
  "peerDependencies": {
    "@vueuse/core": ">13.0.0",
    "@vueuse/shared": ">13.0.0",
    "dayjs": ">=1.11.13 <12",
    "vue": ">=3.5.0"
  },
  "web-types": "dist/web-types.json",
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/types": "7.28.5",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@fontsource/roboto": "5.2.8",
    "@popperjs/core": "2.11.8",
    "@rotki/eslint-config": "4.5.0",
    "@rotki/eslint-plugin": "1.2.0",
    "@storybook/addon-docs": "9.1.16",
    "@storybook/addon-links": "9.1.16",
    "@storybook/addon-themes": "9.1.16",
    "@storybook/vue3-vite": "9.1.16",
    "@tsconfig/node22": "22.0.2",
    "@types/node": "22.18.13",
    "@types/tinycolor2": "1.4.6",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-v8": "4.0.5",
    "@vitest/ui": "4.0.5",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "@vueuse/core": "14.0.0",
    "@vueuse/math": "14.0.0",
    "@vueuse/shared": "14.0.0",
    "argparse": "2.0.1",
    "autoprefixer": "10.4.21",
    "babel-loader": "10.0.0",
    "bumpp": "10.3.1",
    "cac": "6.7.14",
    "consola": "3.4.2",
    "css-loader": "7.1.2",
    "eslint": "9.38.0",
    "eslint-plugin-cypress": "5.2.0",
    "eslint-plugin-storybook": "9.1.16",
    "fast-glob": "3.3.3",
    "fast-xml-parser": "5.3.0",
    "fs-extra": "11.3.2",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.2.6",
    "lucide": "0.541.0",
    "msw": "2.11.6",
    "postcss": "8.5.6",
    "sass": "1.93.2",
    "scule": "1.3.0",
    "storybook": "9.1.16",
    "tailwindcss": "3.4.18",
    "tinycolor2": "1.6.0",
    "tsc-alias": "1.8.16",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "unplugin-auto-import": "20.2.0",
    "vite": "7.1.12",
    "vite-plugin-vue-devtools": "8.0.3",
    "vitest": "4.0.5",
    "vue": "3.5.22",
    "vue-component-meta": "3.1.2",
    "vue-loader": "17.4.2",
    "vue-router": "4.6.3",
    "vue-tsc": "3.1.2"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  }
}
