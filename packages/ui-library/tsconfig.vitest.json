{
  "extends": "./tsconfig.lib.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "types": ["node", "jsdom", "vitest/globals"],
    "skipLibCheck": true
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "tests/**/*"
  ],
  "exclude": [
    "src/**/*.stories.ts"
  ]
}
