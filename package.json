{
  "name": "@rotki/ui-library",
  "version": "0.5.1",
  "packageManager": "pnpm@8.7.6",
  "description": "A vue design system and component library for rotki",
  "type": "module",
  "keywords": [
    "vue",
    "ui",
    "components",
    "rotki"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/rotki/ui-library#readme",
  "bugs": {
    "url": "https://github.com/rotki/ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotki/ui-library.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "/dist"
  ],
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "theme": [
        "./dist/theme/index.d.ts"
      ],
      "composables": [
        "./dist/composables/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./theme": {
      "import": "./dist/theme/index.es.js",
      "default": "./dist/theme/index.es.js",
      "types": "./dist/theme/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.es.js",
      "default": "./dist/components/index.es.js",
      "types": "./dist/components/index.d.ts"
    },
    "./composables": {
      "import": "./dist/composables/index.es.js",
      "default": "./dist/composables/index.es.js",
      "types": "./dist/composables/index.d.ts"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "scripts": {
    "build:prod": "node scripts/dist-build.cjs",
    "build": "vite build",
    "build:storybook": "STORYBOOK=true storybook build",
    "build:tailwind": "tailwindcss -o dist/style.css --minify",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "generate-icons": "node scripts/generate-icons.cjs",
    "resolve:alias": "tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run",
    "test:e2e": "pnpm --filter {example} test:e2e",
    "test:e2e:dev": "pnpm --filter {example} test:e2e:dev",
    "test:all": "pnpm test:run && pnpm test:e2e",
    "coverage": "vitest run --coverage",
    "coverage:preview": "npx vite preview --open --outDir tests/coverage",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install && node scripts/prepare.cjs",
    "release": "bumpp -r --no-push"
  },
  "peerDependencies": {
    "@vueuse/core": ">10.0.0",
    "@vueuse/shared": ">10.0.0",
    "vue": ">=3.3.4"
  },
  "optionalDependencies": {
    "vue-router": ">=4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/types": "7.22.19",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@fontsource/roboto": "5.0.8",
    "@headlessui/vue": "1.7.16",
    "@popperjs/core": "2.11.8",
    "@rotki/eslint-config": "1.1.2",
    "@storybook/addon-essentials": "7.5.0",
    "@storybook/addon-interactions": "7.5.0",
    "@storybook/addon-links": "7.5.0",
    "@storybook/addon-mdx-gfm": "7.5.0",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/addons": "7.5.0",
    "@storybook/blocks": "7.5.0",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3": "7.5.0",
    "@storybook/vue3-vite": "7.5.0",
    "@types/node": "20.2.4",
    "@vitejs/plugin-vue": "4.3.4",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "@vue/test-utils": "2.4.1",
    "@vue/tsconfig": "0.4.0",
    "@vueuse/core": "10.4.1",
    "@vueuse/math": "10.4.1",
    "@vueuse/shared": "10.4.1",
    "argparse": "2.0.1",
    "autoprefixer": "10.4.15",
    "babel-loader": "8.3.0",
    "bumpp": "9.2.0",
    "css-loader": "6.8.1",
    "eslint": "8.49.0",
    "fast-glob": "3.3.1",
    "fast-xml-parser": "4.2.7",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "14.0.1",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remixicon": "3.5.0",
    "sass": "1.67.0",
    "scule": "1.0.0",
    "storybook": "7.5.0",
    "tailwindcss": "3.3.3",
    "tsc-alias": "1.8.7",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.6",
    "vite": "4.4.9",
    "vitest": "0.34.4",
    "vue": "3.3.4",
    "vue-loader": "17.2.2",
    "vue-router": "4.2.4",
    "vue-tsc": "1.8.11"
  },
  "engines": {
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,cjs,ts,vue,yml,json,md}": "eslint"
  }
}
