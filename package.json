{
  "name": "@rotki/ui-library",
  "version": "1.0.0",
  "packageManager": "pnpm@8.1.0",
  "description": "A vue design system and component library for rotki",
  "keywords": [
    "vue",
    "ui",
    "components",
    "rotki"
  ],
  "license": "AGPL-3.0",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "dist:build": "node scripts/dist-build.js",
    "dist:select": "node scripts/dist-select.js",
    "build:prod": "pnpm dist:build && pnpm dist:select",
    "build": "vite build",
    "build:storybook": "storybook build",
    "build:tailwind": "tailwindcss -o dist/style.css --minify",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "resolve:alias": "tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/types": "7.21.5",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@rotki/eslint-config": "1.0.6",
    "@storybook/addon-essentials": "7.0.17",
    "@storybook/addon-interactions": "7.0.17",
    "@storybook/addon-links": "7.0.17",
    "@storybook/addon-styling": "1.0.8",
    "@storybook/blocks": "7.0.17",
    "@storybook/testing-library": "0.1.0",
    "@storybook/vue3": "7.0.17",
    "@storybook/vue3-vite": "7.0.17",
    "@types/node": "20.2.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/tsconfig": "0.4.0",
    "@vueuse/core": "10.1.2",
    "@vueuse/head": "1.1.26",
    "@vueuse/shared": "10.1.2",
    "argparse": "2.0.1",
    "autoprefixer": "10.4.14",
    "babel-loader": "8.3.0",
    "css-loader": "6.7.4",
    "eslint": "8.41.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.62.1",
    "storybook": "7.0.17",
    "tailwindcss": "3.3.2",
    "tsc-alias": "1.8.6",
    "typescript": "5.0.4",
    "unplugin-auto-import": "0.16.2",
    "vite": "4.3.9",
    "vue-loader": "17.1.1",
    "vue-tsc": "1.6.5"
  },
  "engines": {
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,ts,vue,yml,json,md}": "eslint"
  }
}
