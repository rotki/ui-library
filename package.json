{
  "name": "@rotki/ui-library",
  "version": "0.1.2",
  "packageManager": "pnpm@8.6.9",
  "description": "A vue design system and component library for rotki",
  "keywords": [
    "vue",
    "ui",
    "components",
    "rotki"
  ],
  "license": "AGPL-3.0",
  "homepage": "https://github.com/rotki/ui-library#readme",
  "bugs": {
    "url": "https://github.com/rotki/ui-library/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotki/ui-library.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "files": [
    "/dist"
  ],
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./*": {
      "default": "./dist/"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.es.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "default": "./dist/theme.es.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components.es.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "default": "./dist/composables.es.js"
    }
  },
  "scripts": {
    "build:prod": "node scripts/dist-build.cjs",
    "build": "vite build",
    "build:storybook": "STORYBOOK=true storybook build",
    "build:tailwind": "tailwindcss -o dist/style.css --minify",
    "build:types": "vue-tsc -p tsconfig.build.json",
    "generate-icons": "node scripts/generate-icons.cjs",
    "resolve:alias": "tsc-alias -p tsconfig.build.json",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run",
    "test:e2e": "pnpm --filter {example} test:e2e",
    "test:e2e:dev": "pnpm --filter {example} test:e2e:dev",
    "test:all": "pnpm test:run && pnpm test:e2e",
    "coverage": "vitest run --coverage",
    "coverage:preview": "npx vite preview --open --outDir tests/coverage",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install && node scripts/prepare.cjs",
    "release": "bumpp -r --no-push"
  },
  "peerDependencies": {
    "vue": ">=3.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/types": "7.22.5",
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@fontsource/roboto": "5.0.5",
    "@headlessui/vue": "1.7.14",
    "@rotki/eslint-config": "1.1.0",
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/addon-interactions": "7.1.0",
    "@storybook/addon-links": "7.1.0",
    "@storybook/addon-styling": "1.3.4",
    "@storybook/addons": "7.1.0",
    "@storybook/blocks": "7.1.0",
    "@storybook/testing-library": "0.2.0",
    "@storybook/vue3": "7.1.0",
    "@storybook/vue3-vite": "7.1.0",
    "@types/node": "20.2.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "@vue/test-utils": "2.4.0",
    "@vue/tsconfig": "0.4.0",
    "@vueuse/core": "10.2.1",
    "@vueuse/shared": "10.2.1",
    "argparse": "2.0.1",
    "autoprefixer": "10.4.14",
    "babel-loader": "8.3.0",
    "bumpp": "9.1.1",
    "css-loader": "6.8.1",
    "eslint": "8.45.0",
    "fast-xml-parser": "4.2.6",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remixicon": "3.4.0",
    "sass": "1.63.6",
    "scule": "1.0.0",
    "storybook": "7.1.0",
    "tailwindcss": "3.3.3",
    "tsc-alias": "1.8.7",
    "typescript": "5.1.6",
    "unplugin-auto-import": "0.16.6",
    "vite": "4.4.4",
    "vitest": "0.33.0",
    "vue": "3.3.4",
    "vue-loader": "17.2.2",
    "vue-tsc": "1.8.5"
  },
  "engines": {
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,cjs,ts,vue,yml,json,md}": "eslint"
  }
}
